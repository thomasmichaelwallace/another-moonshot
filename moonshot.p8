version 29
__lua__
--another moonshot
--thomas michael wallace

px=32
py=32
mx=1
my=1

function _init()
 px=32
 py=32
end

function _update()
 local x=px
 local y=py
 if(btnp(0))then
  x-=8
 elseif(btnp(1))then
  x+=8
 elseif(btnp(2))then
  y-=8
 elseif(btnp(3))then
  y+=8
 end
 local m=mget((mx-1)*16+x/8,(my-1)*16+y/8)
 if(fget(m,1)==true)then
  _init()
 elseif(fget(m,0)~=true)then
  px=x
  py=y
  if(px<0)then
   mx-=1
   px=15*8
  elseif(px>15*8)then
   px=0
   mx+=1
  elseif(py<0)then
   py=15*8
   my-=1
  elseif(py>15*8)then
   py=0
   my+=1
  end
 end
end

function _draw()
 cls()
 map((mx-1)*16,(my-1)*16,0,0,16,16)
 spr(1,px,py)
end
