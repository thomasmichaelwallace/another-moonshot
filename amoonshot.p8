function draw_fall_seq()
 if(tl.i==1)then
  _draw_map()
  spr(tl.s,tl.x,tl.y)
 end
end

function _update_tit()
 if(tl.m)then
  update_fall_movie()
 else
  update_fall_seq()
 end
end

function _draw_tit()
 if(tl.m)then
  draw_fall_movie()
 else
  draw_fall_seq()
 end
end
