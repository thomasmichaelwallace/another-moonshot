pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
-- another moonshot
-- @thomasmichaelwallace

-->8
-- map

function mapn(n,sx,sy)
 local y=0
 if(n>7)then
  y=16
  n-=8
 end
 map(16*n,y,sx,sy,16,16)
end

mp={
 n=0,--map no.
 x=0,--screen top-left x
 y=0,-- /y
 a={--animations
  t=false,--animating
  n=0,--next map no.
  d=0,--direciton 0⬅️➡️⬆️⬇️3
  x=0,--n/map s/top-left x
  y=0,-- /y
 }
}

function ani_map()
 local s=4--speed
 if(mp.a.t)then
  if(mp.a.d==0)then
   mp.x-=s
   mp.a.x-=s
   if(mp.a.x<=0)mp.a.t=false
  elseif(mp.a.d==1)then
   mp.x+=s
   mp.a.x+=s
   if(mp.a.x>=0)mp.a.t=false
  elseif(mp.a.d==2)then
   mp.y-=s
   mp.a.y-=s
   if(mp.a.y<=0)mp.a.t=false
  elseif(mp.a.d==3)then
   mp.y+=s
   mp.a.y+=s
   if(mp.a.y>=0)mp.a.t=false
  end
  if(mp.a.t==false)then
   mp.x=0
   mp.y=0
   mp.n=mp.a.n
   mp.a.x=0
   mp.a.y=0
  end
 end
end

function move_map(d)
 if(d==0)then--⬅️
  mp.a.n=mp.n+1
  mp.a.x=128
 elseif(d==1)then
  mp.a.n=mp.n-1
  mp.a.x=-128
 elseif(d==2)then
  mp.a.n=mp.n+8
  mp.a.y=128
 elseif(d==3)then
  mp.a.n=mp.n-8
  mp.a.y=-128
 end
 mp.a.t=true
 mp.a.d=d
end

function _update60()
 if(mp.a.t)then
  ani_map()
 else
  if(btnp(0))move_map(1)
  if(btnp(1))move_map(0)
  if(btnp(2))move_map(3)
  if(btnp(3))move_map(2)
 end
end


function _draw()
 cls()
 mapn(mp.n,mp.x,mp.y)
 if(mp.a.t)then
  mapn(mp.a.n,mp.a.x,mp.a.y)
 end
end
